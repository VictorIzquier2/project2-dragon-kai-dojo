<div class="container-classes">
  <h1 class="sensei-h1">Ranking</h1>
  <div class="main-classes">
    <h1><!-- --></h1>
    <div id="ranking" class="list">
      {{#each sensei}}
      <div class="entry entry-sensei">
        <h2 class="position"></h2>
        <img src="{{this.imageUrl}}" alt="{{this.name}}">
        <div class="rank-info">
          <h4>{{this.name}}</h4>
          <div class="frame">
            <img class="level" src="/images/{{this.level}}.jpg" alt="{{this.level}}">
          </div>
          <h4 class="user">{{this.owner.username}}</h4>
        </div>
         <h2>🏆 {{this.standing}}</h2>
      </div>
      {{/each}}
    </div>
  </div>
  <h1><!-- Title --></h1>
  <div class="menu">
    <div class="card dojo">
      <a href="/main">
        <img class="card-image" src="/images/outside-dojo.jpg" alt="outside dojo">
        <h2>Back to Dojo</h2>
      </a>
    </div>
    <div class="card valley">
      <div class="info">
      </div>
      <a href="/fight">
        <img class="card-image" src="/images/look-for-fight.jpg" alt="outside dojo">
        <h2>Back to Fight</h2>
      </a>
    </div>
  </div>
</div>