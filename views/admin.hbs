<div class="container-classes">
  <div class="header">
    <a href="/main"><h4>Main</h4></a>
    <a href="/log-out"><h4>Log-out</h4></a>
  </div>
  <h1 class="sensei-h1">Admin</h1>
  <div class="main-classes">
    <h1>Senseis</h1>
    <div class="list">
      {{#each senseis}}
      <div class="entry">
        <img src="{{imageUrl}}" alt="{{name}}">
        <h2>{{name}}</h2>
        <img class="sensei-img" src="/images/{{level}}.jpg" alt="{{level}}">
         <h2>🏆 {{standing}}</h2>
      </div>
      {{/each}}
    </div>
  </div>
  <h1><!-- Title --></h1>
  <div class="main-menu">
    <div class="card">
      <div class="info">
        <h3>{{{civilians}}}</h3>
      </div>
      <img class="card-image" src="/images/crowd.jpg" alt="civilians">
      <form action="/admin/civilians" method="GET">
        <button class="btn btn-primary">Civilians</button>
      </form>
    </div>
    <div class="card">
      <div class="info">
        <h3>{{{karatekas}}}</h3>
      </div>
      <img class="card-image" src="/images/train.jpg" alt="karatekas">
      <form action="/admin/karatekas" method="GET">
        <button class="btn btn-primary">Karatekas</button>
      </form>
    </div>
    <div class="card">
      <div class="info">
        <h3>{{{masters}}}</h3>
      </div>
      <img class="card-image" src="/images/tourney.jpg" alt="outside dojo">
      <form action="/admin/masters" method="GET">
        <button class="btn btn-primary">Masters</button>
      </form>
    </div>
  </div>
  <h1><!-- --></h1>
  <div class="menu">
    <div class="card dojo">
      <a href="/main">
        <img class="card-image" src="/images/outside-dojo.jpg" alt="outside dojo">
        <h2>Back to Dojo</h2>
      </a>
    </div>
    <div class="card valley">
      <div class="info">
      </div>
      <a href="/city">
        <img class="card-image" src="/images/city.jpg" alt="outside dojo">
        <h2>Go to All Valley</h2>
      </a>
    </div>
  </div>
</div>