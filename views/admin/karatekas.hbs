<div class="container-classes">
  <h1>Karatekas</h1>
  <div class="menu">
    <div class="card valley">
      <div class="info">
      </div>
      <a href="/admin">
        <img class="card-image" src="/images/admin.jpg" alt="admin">
        <h2>Back to Admin</h2>
      </a>
    </div>
    <div class="card dojo">
      <a href="/main">
        <img class="card-image" src="/images/outside-dojo.jpg" alt="outside dojo">
        <h2>Back to Dojo</h2>
      </a>
    </div>
  </div>
  <h1><!-- Title --></h1>
  <div class="main-classes">
    <div class="list">
      {{#each karatekas}}
       <div class="entry">
        <input class="input-karatekas" type="checkbox" id="{{_id}}" name="_id" value="{{_id}}" onchange="countOppSelected(this)">
        <img src="{{imageUrl}}" alt="{{name}}">
        <a class="edit" href="/admin/karatekas/{{_id}}"><i class="fas fa-edit"></i></a>
        <label for="{{_id}}">{{name}}</label>
        <img src="/images/{{level}}.jpg" alt="{{level}}">
        <h2>ğŸ† {{standing}}</h2>
      </div>
      {{/each}}
    </div>
  </div>
  
</div>