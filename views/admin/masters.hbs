<div class="container-classes">
  <h1>Masters</h1>
  <div class="menu">
    <div class="card valley">
      <div class="info">
      </div>
      <a href="/admin">
        <img class="card-image" src="/images/admin.jpg" alt="admin">
        <h2>Back to Admin</h2>
      </a>
    </div>
    <div class="card dojo">
      <a href="/main">
        <img class="card-image" src="/images/outside-dojo.jpg" alt="outside dojo">
        <h2>Back to Dojo</h2>
      </a>
    </div>
  </div>
  <div class="main-menu">
    <div class="card">
      <div class="info">
        <h3>{{{mastersNumber}}}</h3>
      </div>
      <img class="card-image" src="/images/tourney.jpg" alt="Create master">
      <form action="/admin/masters" method="POST">
        <button class="btn btn-primary">Create</button>
      </form>
    </div>
  </div>
  <h1><!-- Title --></h1>
  <div class="main-classes">
    <div class="list">
      {{#each masters}}
       <div class="entry">
        <input class="input-masters" type="checkbox" id="{{_id}}" name="_id" value="{{_id}}">
        <img src="{{imageUrl}}" alt="{{name}}">
        <a class="edit" href="/admin/masters/{{_id}}"><i class="fas fa-edit"></i></a>
        <a class="delete" href="/admin/masters/delete/{{_id}}"><i class="fas fa-trash"></i></a>
        <label for="{{_id}}">{{name}}</label>
        <img src="/images/{{level}}.jpg" alt="{{level}}">
        <h2>ğŸ† {{standing}}</h2>
      </div>
      {{/each}}
    </div>
  </div>
</div>